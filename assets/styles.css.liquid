  border: 1px solid #E9E9E9;
}

.light-theme .next {
  margin-left: 30px;
}

.light-theme .prev {
  margin-right: 30px;
}

.light-theme .current {
  color: #000;
  border: 1px solid #E9E9E9;
  cursor: default;
  border-radius: 4px;
}

.light-theme span.prev.current,
.light-theme span.current.next {
