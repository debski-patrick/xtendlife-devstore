.form-check-input, .form-radio-input {
  position: relative;
  cursor: pointer;
  padding: 1em;
}
.form-check-input input, .form-radio-input input {
  position: absolute;
  cursor: pointer;
  opacity: 0;
}
.form-check-input input + span, .form-radio-input input + span {
  background: white;
  content: "";
  display: inline-block;
  margin: 0 1em 0 0;
  padding: 0;
  vertical-align: middle;
  width: 2em;
  height: 2em;
  transform: translateZ(0);
  -webkit-backface-visibility: hidden;
  border: 1px solid #1b3281;
  border-radius: 4px;
}
.form-check-input input + span::after, .form-radio-input input + span::after {
  content: "";
  display: block;
  transform: scale(0);
  transition: transform .2s;
}
.form-check-input input + span::after {
  background: url(data:image/svg+xml;base64,) no-repeat center;
  background-size: contain;
  width: 2em;
  height: 2em;
}
.form-check-input input:checked + span::after, .form-radio-input input:checked + span::after {
  transform: scale(1);
}
